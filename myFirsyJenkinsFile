node ('master'){
    try{

        stage ('checkout'){
            checkout scm
        }

        stage ('Test'){
            sh 'docker run --rm -v "$(pwd)":/sitespeed.io sitespeedio/sitespeed.io:10.1.0 https://www.sitespeed.io/'
        }

    } catch (Exception e) {
        println (e.getMessage())
    } finally {
        echo '$(currentBuild.result)'
    }
}